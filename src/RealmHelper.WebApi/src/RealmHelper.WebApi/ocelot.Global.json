{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{path}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "{Bedrock}"
        }
      ],
      "UpstreamPathTemplate": "/bedrock/{path}",
      "UpstreamHttpMethod": ["Get", "Post", "Patch", "Put", "Delete"]
    },
    {
      "DownstreamPathTemplate": "/{path}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "{Java}"
        }
      ],
      "UpstreamPathTemplate": "/java/{path}",
      "UpstreamHttpMethod": ["Get", "Post", "Patch", "Put", "Delete"]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "[Base URL]"
  }
}